<keymap version="1" name="Default copy" parent="$default">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="Back">
    <keyboard-shortcut first-keystroke="ctrl alt left" />
  </action>
  <action id="CodeCompletion">
    <keyboard-shortcut first-keystroke="alt slash" />
  </action>
  <action id="Editor Redo">
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="EditorDeleteLine" />
  <action id="HippieBackwardCompletion" />
  <action id="HippieCompletion" />
  <action id="Images.SetBackgroundImage">
    <keyboard-shortcut first-keystroke="ctrl alt numpad9" />
  </action>
  <action id="IntroduceVariable">
    <keyboard-shortcut first-keystroke="ctrl alt v" />
    <keyboard-shortcut first-keystroke="ctrl numpad5" />
  </action>
  <action id="SearchEverywhere">
    <mouse-shortcut keystroke="button4" />
  </action>
  <action id="SmartTypeCompletion">
    <keyboard-shortcut first-keystroke="shift alt slash" />
  </action>
</keymap>